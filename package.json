    "url": "https://github.com/makeomatic/redux-universal/issues"
  },
  "homepage": "https://github.com/makeomatic/redux-universal#readme",
  "dependencies": {
    "axios": "^0.8.1",
    "bluebird": "^3.1.1",
    "history": "^1.13.1",
    "lodash": "^3.10.1",
    "lodash.forown": "^3.0.2",
    "react": "^0.14.3",
    "react-document-meta": "^2.0.0",
    "react-dom": "^0.14.3",
