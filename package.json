    "minifyify": "^7.1.0",
    "mocha": "^2.3.3",
    "react-transform-hmr": "^1.0.1",
    "restify": "^4.0.3",
    "rev-del": "^1.0.5",
    "vinyl-source-stream": "^1.1.0"
