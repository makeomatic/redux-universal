{
  "name": "redux-universal",
  "version": "0.1.0",
  "description": "Cappasity JSON:API restify server",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "if [[ $NODE_ENV != \"production\" ]]; then ./node_modules/.bin/babel -d ./lib ./src; fi",
    "react": "if [[ $NODE_ENV != \"production\" ]]; then NODE_ENV=production ./node_modules/.bin/gulp bundle; fi",
    "prepublish": "npm run compile && npm run react",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "./node_modules/.bin/babel-node ./bin/restify.js | ./node_modules/.bin/bunyan -o short",
    "start-dev": "gulp watch | ./node_modules/.bin/bunyan -o short"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makeomatic/redux-universal.git"
