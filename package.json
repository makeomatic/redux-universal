    "babel-eslint": "^5.0.0-beta4",
    "babelify": "^6.4.0",
    "browserify": "^12.0.1",
    "browserify-shim": "^3.8.11",
    "bundle-collapser": "^1.2.1",
    "bunyan": "^1.5.1",
    "chai": "^3.4.1",
    "ecstatic": "^1.3.1",
